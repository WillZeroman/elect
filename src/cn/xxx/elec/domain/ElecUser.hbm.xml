<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.xxx.elec.domain.ElecUser" table="elec_user">
	<id name="userID" type="string">
		<column name="userID" ></column>
		<generator class="uuid"></generator>
	</id>
	<property name="jctID" type="string">
		<column name="jctID"></column>
	</property>	
	<property name="userName" type="string">
		<column name="userName"></column>
	</property>
	<property name="loginName" type="string">
		<column name="loginName"></column>
	</property>
	<property name="loginPwd" type="string">
		<column name="loginPwd"></column>
	</property>
		<property name="sexID" type="string">
		<column name="sexID"></column>
	</property>
	<property name="birthday" type="date">
		<column name="birthday" sql-type="datetime"></column>
	</property>
	<property name="address" type="string">
		<column name="address"></column>
	</property>
	<property name="contactTel" type="string">
		<column name="contactTel"></column>
	</property>
		<property name="email" type="string">
		<column name="email"></column>
	</property>
		<property name="mobile" type="string">
		<column name="mobile"></column>
	</property>
	<property name="isDuty" type="string">
		<column name="isDuty"></column>
	</property>
	<property name="onDutyDate" type="date">
		<column name="onDutyDate" sql-type="datetime"></column>
	</property>
	<property name="offDutyDate" type="date">
		<column name="offDutyDate" sql-type="datetime"></column>
	</property>
	<property name="remark" type="string">
		<column name="remark"></column>
	</property>
	</class>
</hibernate-mapping>