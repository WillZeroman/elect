<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.xxx.elec.domain.ElecRolePopedom" table="elec_role_popedom">
		<id name="roleID" type="string">
			<column name="roleID" not-null="true"></column>
		</id>
		<property name="popedomcode" type="string">
			<column name="popedomcode"></column>
		</property>	
		<property name="remark" type="string">
			<column name="remark"></column>
		</property>
	</class>
</hibernate-mapping>